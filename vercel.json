{
"builds": [
    {
      "src": "/api/main.py",
      "use": "@vercel/python"
    }]
  ,
  "routes": [
       { "src": "/:user/:project/archive/refs/heads/:files.:zip", "dest": "https://codeload.github.com/:user/zip/refs/heads/:files" },
    { "src": "/(.*)", "dest": "/api/main.py" }
       
  ]
}
