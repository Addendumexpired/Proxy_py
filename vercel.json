{
"builds": [
    {
      "src": "/api/main.py",
      "use": "@vercel/python"
    }]
  ,
  "routes": [
     { "src": "/:user/:project/archive/refs/heads/:version.:zip", "dest": "https://codeload.github.com/:user/:project/zip/refs/heads/:version" },
    { "src": "/(.*)", "dest": "https://github.com/" }
       
  ]
}
